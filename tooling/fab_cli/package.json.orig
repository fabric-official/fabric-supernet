{
    "name": "@fabric_v1/fab-cli",
    "version": "1.0.5",
    "description": "Command-line interface for Fabric DSL (fab)",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "fab": "dist/index.js"
    },
    "scripts": {
        "build": "tsc && node ./scripts/copy-schema.js",
        "lint": "eslint \"src/**/*.ts\"",
        "type-check": "tsc --noEmit",
        "test": "jest --passWithNoTests",
        "coverage": "jest --coverage",
        "prepublishOnly": "npm run build && npm run lint && npm test"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/",
        "README.md",
        "LICENSE.md"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "dependencies": {
        "@fabric_v1/compiler": "^1.0.2",
        "@grpc/grpc-js": "^1.13.4",
        "@grpc/proto-loader": "^0.8.0",
        "ajv": "^8.17.1",
        "ajv-formats": "^2.1.1",
        "axios": "^1.11.0",
        "chalk": "^4.1.0",
        "commander": "^10.0.0",
        "fs-extra": "^11.3.0",
        "inquirer": "^9.0.0",
        "js-yaml": "^4.1.0",
        "node-fetch": "^2.6.7",
        "odbc": "^2.3.0",
        "openai": "^4.0.0",
        "peggy": "^2.0.0"
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
        "@types/fs-extra": "^11.0.4",
        "@types/inquirer": "^9.0.0",
        "@types/jest": "^29.0.0",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^18.19.120",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^8.37.0",
        "@typescript-eslint/parser": "^8.37.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-import": "^2.0.0",
        "jest": "^29.0.0",
        "prettier": "^2.0.0",
        "ts-jest": "^29.0.0",
        "typescript": "^5.0.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src"
        ]
    },
    "engines": {
        "node": ">=18"
    },
    "license": "Apache-2.0"
}
